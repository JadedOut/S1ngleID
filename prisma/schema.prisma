generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Credential {
  id            String   @id @default(cuid())
  credentialId  String   @unique
  publicKey     String
  counter       Int      @default(0)
  status        String
  issuedAt      DateTime @default(now())
  expiresAt     DateTime?
}